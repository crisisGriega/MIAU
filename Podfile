platform :ios, '11.0'
use_frameworks!

def common_pods
    pod 'Alamofire'
    pod 'AlamofireObjectMapper'
end

target 'MIAU' do
    common_pods
    pod 'AlamofireImage'
    pod 'DSGradientProgressView'
    pod 'AKPickerView-Swift', :git => 'https://github.com/crisisGriega/AKPickerView-Swift.git'
end

target 'MIAUTests' do
    common_pods
end
